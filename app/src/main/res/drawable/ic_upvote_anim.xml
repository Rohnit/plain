<?xml version="1.0" encoding="utf-8"?>

<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportHeight="24"
    android:viewportWidth="24">

    <clip-path
        android:pathData="@string/path_upvote_clip" />

    <path
        android:name="empty"
        android:pathData="@string/path_upvote"
        android:strokeColor="@color/text_secondary_dark"
        android:strokeWidth="2"
        android:strokeLineJoin="round" />

    <group
        android:name="filled">

        <path
            android:pathData="@string/path_upvote"
            android:strokeColor="@color/designer_news"
            android:strokeWidth="2"
            android:strokeLineJoin="round"
            android:fillColor="@color/designer_news"/>

    </group>

</vector>
